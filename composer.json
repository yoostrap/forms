{
    "name": "hizzle/forms",
    "type": "library",
    "description": "Simple forms plugin.",
    "keywords": ["wordpress","hizzle","forms"],
    "homepage": "http://hizzle.co/forms/",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Lewis Ushindi",
            "email": "frenziecodes@gmail.com",
            "homepage": "http://github.com/frenziecodes/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hizzle\\Forms\\": ["src/", "pro/"]
        },
        "classmap": [
            "src/",
            "pro/"
        ]
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
        }
    },
    "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "2.*"
    },
    "config": {
    "allow-plugins": {
        "composer/installers": true
    }
    },
    "require": {
        "composer/installers": "^1.12",
        "php": ">=5.3.0"
    }
}
